<div class="panel">
  <div class="panel-header">
    <h4>Users</h4>
  </div>
  <div class="panel-body">
    <% @users.each do |user| %>
      <% if !(user.admin?) %>
      <div class="row">
        <div class="col-xs-2">
          <%= user.name %>
        </div>
        <div class="col-xs-2">
          <%= user.email %>
        </div>
        <div class="col-xs-2">
          <%= user.telephone %>
        </div>
        <div class="col-xs-2">
          <%= link_to "Destroy", user, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-default" %>
        </div>
      </div>
      <br>
      <% end %>
    <% end %>
  </div>
</div>



